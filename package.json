{
  "name": "dv-stip-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "g": "nx generate @dv/tooling/nx-plugin:lib",
    "g:dry": "nx generate @dv/tooling/nx-plugin:lib --dry-run",
    "g:app": "nx generate @dv/tooling/nx-plugin:app",
    "g:app:dry": "nx generate @dv/tooling/nx-plugin:app --dry-run",
    "validate": "nx generate @dv/tooling/nx-plugin:validate",
    "validate:fix": "nx generate @dv/tooling/nx-plugin:validate --fix",
    "graph": "nx dep-graph --groupByFolder",
    "readme:toc": "markdown-toc -i README.md",
    "reset": "nx reset",
    "start": "run-p serve:gesuch-app serve:gesuch-app:mock",
    "start:backend": "nx serve gesuch-app --proxyConfig proxy.conf.json",
    "serve:gesuch-app": "nx serve gesuch-app --proxyConfig proxy.conf.json",
    "serve:gesuch-app:mock": "json-server --watch mock/db.json --delay 300 --port 8080 --routes mock/db-routes.json",
    "lint:gesuch-app": "nx lint gesuch-app",
    "test:gesuch-app": "nx test gesuch-app",
    "e2e:gesuch-app": "nx e2e gesuch-app-e2e",
    "e2e:gesuch-app:open": "npx cypress open --project apps/gesuch-app-e2e --config baseUrl=http://localhost:4200",
    "build:gesuch-app": "nx build gesuch-app",
    "analyze:gesuch-app": "nx build gesuch-app --output-hashing=none --named-chunks --source-map && source-map-explorer dist/apps/gesuch-app/*.js --html dist/apps/gesuch-app/sme/index.html && http-server ./dist/apps/gesuch-app/sme/",
    "test:tooling-nx-plugin": "nx run tooling-nx-plugin:test",
    "lint": "nx run-many --target=lint",
    "test": "nx run-many --target=test",
    "e2e": "nx run-many --target=e2e",
    "build": "nx run-many --target=build",
    "format:test": "nx format:check --all",
    "format:write": "nx format:write --all",
    "openapi": "ts-node scripts/openapi-build.ts",
    "openapi:extract-current-templates": "openapi-generator-cli author template -g typescript-angular -o scripts/conf/openapi-templates/default-6.5.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.0.0",
    "@angular/cdk": "^16.0.2",
    "@angular/common": "~16.0.0",
    "@angular/compiler": "~16.0.0",
    "@angular/core": "~16.0.0",
    "@angular/forms": "~16.0.0",
    "@angular/localize": "^16.0.3",
    "@angular/platform-browser": "~16.0.0",
    "@angular/platform-browser-dynamic": "~16.0.0",
    "@angular/router": "~16.0.0",
    "@maskito/angular": "^0.12.1",
    "@maskito/core": "^0.12.1",
    "@ng-bootstrap/ng-bootstrap": "^15.0.0",
    "@ngrx/component-store": "^16.0.1",
    "@ngrx/effects": "^16.0.0",
    "@ngrx/entity": "^16.0.0",
    "@ngrx/router-store": "^16.0.0",
    "@ngrx/store": "^16.0.0",
    "@ngrx/store-devtools": "^16.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@popperjs/core": "^2.11.7",
    "bootstrap": "^5.2.3",
    "date-fns": "^2.30.0",
    "ngx-translate-multi-http-loader": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.0",
    "@angular-devkit/core": "~16.0.0",
    "@angular-devkit/schematics": "~16.0.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "~16.0.0",
    "@angular/language-service": "~16.0.0",
    "@ngrx/eslint-plugin": "^16.0.0",
    "@nx/angular": "^16.1.4",
    "@nx/cypress": "16.1.4",
    "@nx/devkit": "^16.1.3",
    "@nx/eslint-plugin": "16.1.1",
    "@nx/jest": "16.3.2",
    "@nx/js": "16.3.2",
    "@nx/linter": "16.1.1",
    "@nx/plugin": "^16.1.2",
    "@nx/web": "16.1.4",
    "@nx/workspace": "16.1.1",
    "@schematics/angular": "~16.0.0",
    "@swc-node/register": "~1.4.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@swc/helpers": "^0.5.1",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cypress": "^12.11.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "handlebars": "^4.7.7",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "inquirer": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "^13.1.1",
    "json-server": "^0.17.3",
    "jsonc-eslint-parser": "^2.2.0",
    "lint-staged": "^13.2.2",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "nx": "16.1.1",
    "prettier": "^2.6.2",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.0.2"
  }
}
