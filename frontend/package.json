    "npm-run-all": "^4.1.5",
    "nx": "20.0.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
