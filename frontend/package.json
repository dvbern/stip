    "@angular-devkit/build-angular": "18.1.4",
    "@angular-devkit/core": "18.1.4",
    "@angular-devkit/schematics": "18.1.4",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "~18.1.0",
    "@angular/compiler-cli": "18.1.4",
    "@angular/language-service": "18.1.4",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@ngrx/eslint-plugin": "18.0.2",
    "@nx/angular": "19.5.7",
    "@nx/devkit": "19.5.7",
    "@nx/eslint": "19.5.7",
    "@nx/eslint-plugin": "19.5.7",
    "@nx/jest": "19.5.7",
    "@nx/js": "19.5.7",
    "@nx/playwright": "19.5.7",
    "@nx/plugin": "19.5.7",
    "@nx/web": "19.5.7",
    "@nx/workspace": "19.5.7",
    "@playwright/test": "^1.46.0",
    "@schematics/angular": "18.1.4",
    "@stoplight/prism-cli": "^5.9.0",
    "@swc-node/register": "1.9.2",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.12",
    "@testing-library/angular": "^17.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "22.3.0",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/utils": "^7.16.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "handlebars": "^4.7.8",
    "html-webpack-plugin": "^5.6.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "14.2.2",
    "json-server": "^0.17.4",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^15.2.9",
    "ngx-translate-testing": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "nx": "19.5.7",
    "prettier": "^3.3.3",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "**/*.{ts,html}": [
      "npx prettier -w"
    ]
  }
}
