{
  "name": "@dv/stip-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "cd .. && husky frontend/.husky",
    "start": "nx run-many --target=serve",
    "start:gs": "npm run serve:gs",
    "start:sb": "npm run serve:sb",
    "start:soz": "npm run serve:soz",
    "serve:gs": "nx serve gesuch-app",
    "serve:sb": "nx serve sachbearbeitung-app",
    "serve:soz": "nx serve sozialdienst-app",
    "serve:demo": "nx serve demo-data-app",
    "lint": "eslint . --plugin file-progress --rule 'file-progress/activate: 1' --cache --cache-location node_modules/.eslintcache",
    "lint:without-progress": "eslint . --cache --cache-location node_modules/.eslintcache",
    "pretest": "tsc -p tsconfig.spec.json --noEmit",
    "test": "vitest run",
    "test:changed": "npm run test -- --changed HEAD^",
    "test:ci": "npm run test -- --coverage",
    "e2e": "nx run-many --target=e2e",
    "e2e:gs": "nx e2e gesuch-app-e2e",
    "e2e:sb": "nx e2e sachbearbeitung-app-e2e",
    "e2e:soz": "nx e2e sozialdienst-app-e2e",
    "build": "nx run-many --target=build",
    "build:gs": "nx build gesuch-app",
    "prebuild:gs": "npm run set-version",
    "build:sb": "nx build sachbearbeitung-app",
    "prebuild:sb": "npm run set-version",
    "build:soz": "nx build sozialdienst-app",
    "prebuild:soz": "npm run set-version",
    "g": "nx generate @dv/tooling/nx-plugin:lib",
    "g:dry": "nx generate @dv/tooling/nx-plugin:lib --dry-run",
    "g:app": "nx generate @dv/tooling/nx-plugin:app",
    "g:app:dry": "nx generate @dv/tooling/nx-plugin:app --dry-run",
    "validate": "nx generate @dv/tooling/nx-plugin:validate",
    "validate:fix": "nx generate @dv/tooling/nx-plugin:validate --fix",
    "graph": "nx dep-graph --groupByFolder",
    "reset": "nx reset",
    "analyze:gs": "nx build gesuch-app --output-hashing=none --named-chunks --source-map && source-map-explorer dist/apps/gesuch-app/**/*.js --html dist/apps/gesuch-app/sme/index.html && http-server ./dist/apps/gesuch-app/sme/",
    "analyze:sb": "nx build sachbearbeitung-app --output-hashing=none --named-chunks --source-map && source-map-explorer dist/apps/sachbearbeitung-app/**/*.js --html dist/apps/sachbearbeitung-app/sme/index.html && http-server ./dist/apps/sachbearbeitung-app/sme/",
    "analyze:soz": "nx build sozialdienst-app --output-hashing=none --named-chunks --source-map && source-map-explorer dist/apps/sozialdienst-app/**/*.js --html dist/apps/sozialdienst-app/sme/index.html && http-server ./dist/apps/sozialdienst-app/sme/",
    "format:test": "nx format:check --all",
    "format:write": "nx format:write --all",
    "openapi": "ts-node scripts/openapi-build.ts",
    "openapi:extract-current-templates": "openapi-generator-cli author template -g typescript-angular -o scripts/conf/openapi-templates/default-6.5.0",
    "set-version": "ts-node scripts/version-from-env.ts",
    "sync-roles": "npx tsx scripts/keycloak/update.ts sync-roles",
    "add-users": "npx tsx scripts/keycloak/update.ts add-users"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.1.8",
    "@angular/cdk": "20.1.7",
    "@angular/common": "20.1.8",
    "@angular/compiler": "20.1.8",
    "@angular/core": "20.1.8",
    "@angular/forms": "20.1.8",
    "@angular/localize": "20.1.8",
    "@angular/material": "20.1.7",
    "@angular/material-date-fns-adapter": "20.1.7",
    "@angular/platform-browser": "20.1.8",
    "@angular/platform-browser-dynamic": "20.1.8",
    "@angular/router": "20.1.8",
    "@inquirer/prompts": "^7.8.4",
    "@maskito/angular": "^3.10.3",
    "@maskito/core": "^3.10.3",
    "@maskito/kit": "^3.10.3",
    "@ng-select/ng-select": "^20.2.0",
    "@ngrx/component-store": "20.0.1",
    "@ngrx/effects": "20.0.1",
    "@ngrx/entity": "20.0.1",
    "@ngrx/operators": "20.0.1",
    "@ngrx/router-store": "20.0.1",
    "@ngrx/signals": "20.0.1",
    "@ngrx/store": "20.0.1",
    "@ngrx/store-devtools": "20.0.1",
    "@openapitools/openapi-generator-cli": "^2.23.1",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/postcss": "^4.1.13",
    "angular-oauth2-oidc": "^20.0.2",
    "bootstrap": "^5.3.8",
    "date-fns": "^3.6.0",
    "deepmerge-ts": "^7.1.5",
    "filesize": "^11.0.2",
    "fuse.js": "^7.1.0",
    "ibantools": "^4.5.1",
    "json-diff-ts": "^4.3.0",
    "jspdf": "^3.0.2",
    "libphonenumber-js": "^1.12.15",
    "ngx-mat-select-search": "^8.0.2",
    "rxjs": "~7.8.2",
    "seedrandom": "^3.0.5",
    "tslib": "^2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "1.19.4",
    "@analogjs/vitest-angular": "1.19.4",
    "@angular-devkit/build-angular": "20.1.6",
    "@angular-devkit/core": "20.1.6",
    "@angular-devkit/schematics": "20.1.6",
    "@angular-eslint/eslint-plugin": "20.2.0",
    "@angular-eslint/eslint-plugin-template": "20.2.0",
    "@angular-eslint/template-parser": "20.2.0",
    "@angular/build": "20.1.6",
    "@angular/cli": "~20.1.0",
    "@angular/compiler-cli": "20.1.8",
    "@angular/language-service": "20.1.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "~9.35.0",
    "@jscutlery/swc-angular": "^0.20.5",
    "@jscutlery/swc-angular-plugin": "^0.20.5",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@jsverse/transloco": "^7.6.1",
    "@keycloak/keycloak-admin-client": "^26.3.3",
    "@ngrx/eslint-plugin": "20.0.1",
    "@nx/angular": "21.4.1",
    "@nx/devkit": "21.4.1",
    "@nx/eslint": "21.4.1",
    "@nx/eslint-plugin": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/playwright": "21.4.1",
    "@nx/plugin": "21.4.1",
    "@nx/vite": "21.4.1",
    "@nx/web": "21.4.1",
    "@nx/workspace": "21.4.1",
    "@playwright/test": "^1.55.0",
    "@schematics/angular": "20.2.2",
    "@stoplight/prism-cli": "^5.14.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "^1.10.0",
    "@swc/helpers": "0.5.17",
    "@testing-library/angular": "^18.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "24.3.1",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@typescript-eslint/utils": "^8.33.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "angular-eslint": "20.2.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-file-progress": "^3.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-prettier": "^5.5.4",
    "handlebars": "^4.7.8",
    "html-webpack-plugin": "^5.6.4",
    "http-encoding": "^2.1.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "json-server": "^0.17.4",
    "jsonc-eslint-parser": "^2.4.0",
    "lint-staged": "^16.1.6",
    "nx": "21.4.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4.1.13",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "**/*.{ts,html}": [
      "npx prettier -w"
    ]
  }
}
