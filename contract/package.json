{
  "name": "stip-api",
  "version": "1.0.0",
  "dependencies": {
    "@redocly/cli": "2.11.1"
  },
  "private": true,
  "scripts": {
    "start": "openapi preview",
    "build": "openapi join openapi/*.yaml -o openapi.yaml --without-x-tag-groups",
    "build:no-output": "openapi join openapi/*.yaml --without-x-tag-groups",
    "debug": "openapi bundle openapi/*.yaml -o openapi/generated",
    "lint": "openapi lint openapi/*.yaml"
  }
}
