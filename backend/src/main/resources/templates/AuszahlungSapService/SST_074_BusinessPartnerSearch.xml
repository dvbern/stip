<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="urn:be.ch:KTBE_MDG:BUSINESS_PARTNER" xmlns:s="http://schemas.xmlsoap.org/soap/envelope/">
  <soapenv:Header>
