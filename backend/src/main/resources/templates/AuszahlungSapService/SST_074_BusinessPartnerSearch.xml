      <FILTER_PARMS xmlns="">
        <ONLY_WITH_REF>false</ONLY_WITH_REF>
        <READ_ALL_DETAILS>false</READ_ALL_DETAILS>
        <!--Optional:-->
        <OFFSET>0</OFFSET>
        <!--Optional:-->
